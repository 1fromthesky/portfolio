.feedback {
   min-height: 800px;

   &__title-row {
      display: flex;
      align-items: center;
   }

   &__title {
      font-size: 36px;
      font-weight: 700;
   }

   &__toggles {
      display: flex;

      &-btn {
         @include circle(50px, 1px, $text-color);
         display: block;
         margin: 0 10px;
         background-color: initial;
         position: relative;

      }
   }
}
